<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone.app.dexterity">

<head>
  <div metal:fill-slot="javascript_head_slot" tal:omit-tag="">
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/communityVariables.js">
    </script>
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/++maxui++static/maxui.loader.js">
    </script>
  </div>
  <metal:block fill-slot="top_slot"
                 tal:define="dummy python:request.set('disable_border',1)" />
</head>

<body>

<metal:main fill-slot="main">

  <metal:content-core define-macro="main">

    <div class="navbar">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li><a href="#" data-toggle="tab">Activitat</a></li>
              <li><a data-toggle="tab" tal:attributes="href string:${context/absolute_url}/documents/++add++File">Document</a></li>
              <li><a data-toggle="tab" tal:attributes="href string:${context/absolute_url}/enllacos/++add++Link">Enlla√ß</a></li>
              <li><a data-toggle="tab" tal:attributes="href string:${context/absolute_url}/fotos/++add++Image">Foto</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <div id="maxui-widget-container"></div>

    <div id="documents-view"></div>
    <div id="links-view"></div>
    <div id="images-view"></div>

  </metal:content-core>
</metal:main>

</body>
</html>
